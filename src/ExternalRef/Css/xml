  
  const canvas = <HTMLCanvasElement> document.getElementById('myChart');
  const ctx = canvas.getContext('2d');
  var chart = new Chart(ctx, {
    // The type of chart we want to create
    type: 'bar',

    // The data for our dataset
    data: {
        labels: ['Madhesh Maasi'], 
        datasets: [{
            label: 'Leads Target',
            backgroundColor: 'rgb(255, 99, 132)',
            borderColor: 'rgb(255, 99, 132)',
            data: [LeadsTarget[0].TargetValue]
        },{
          label: 'Leads Acheivements',
          backgroundColor: 'rgb(110, 187, 111)',
          borderColor: 'rgb(110, 187, 111)',
          data: [madLeads.length,0]
      },]
    },

    // Configuration options go here
    options: {}
});
const canvas1 = <HTMLCanvasElement> document.getElementById('myChart-2');
const ctx1 = canvas1.getContext('2d');
var chart1 = new Chart(ctx1, {
  // The type of chart we want to create
  type: 'bar',

  // The data for our dataset
